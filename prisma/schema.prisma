generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model User_Details{
  userId Int @id @default(autoincrement())
  firstName String
  lastName String
  email String @unique
  password String
  phoneNumber String
  address String
  isAdmin Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime
}